{
  "version": 2,
  "builds": [
    {
      "src": "api/__init__.py",
      "use": "@vercel/python"
    },
    {
      "src": "static/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/__init__.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
